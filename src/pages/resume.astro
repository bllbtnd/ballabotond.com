---
import Redirect from '../components/Redirect.astro';
import { defaultLang, useTranslations } from '../i18n/utils';

const t = useTranslations(defaultLang);
const redirectUrl = `/${defaultLang}/resume/`;
const title = t('resume.title');
const description = t('resume.summary.text');
const image = '/assets/images/image.png';
---

<Redirect 
  to={redirectUrl} 
  title={title}
  description={description}
  image={image}
/>
