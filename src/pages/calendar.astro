---
import Redirect from '../components/Redirect.astro';
import { defaultLang, useTranslations } from '../i18n/utils';

const t = useTranslations(defaultLang);
const redirectUrl = `/${defaultLang}/calendar/`;
const title = t('calendar.title');
const description = t('calendar.description');
---

<Redirect 
  to={redirectUrl} 
  title={title}
  description={description}
/>
